<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Daniel James Thorne</Authors>
    <Company>Meonsoft</Company>
    <Description>Google ReCaptcha challenge server-side integration; response verification via http client.  Just use static method `GoogleReCaptcha.VerifyAsync(secret, response,remoteip,endpoint)`.  The last two parameters have default values.</Description>
    <PackageProjectUrl></PackageProjectUrl>
    <Copyright />
    <RepositoryUrl>https://github.com/rombethor/Meonsoft.Integrations.ReCaptcha</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Version>1.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="5.0.2" />
  </ItemGroup>

</Project>
